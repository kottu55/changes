<h1>posts/index</h1>

<% @posts.each do |post| %>
<p>
    <%= link_to post_path(post.id) do %>
    <%= attachment_image_tag post, :before_img, :fill, 100, 100, format: 'jpeg' %>
    <% end %>
    <%= post.user_id %>
    <%= post.title %>
    <%= post.caption %>
    <%= post.time_length %>
    <%= post.category_id %>
    <%= post.owned_tag_id %>
</p>
<% end %>
<br>
<%= link_to "ユーザー一覧", users_path %>
<%= link_to "投稿", new_post_path %>
